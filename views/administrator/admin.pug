extends ../layout

block content

  include ../navbar

  // Masthead
  header.masthead.bg-primary.text-white.text-center
    .container.d-flex.align-items-center.flex-column
      // Masthead Heading
      h1.masthead-heading.text-uppercase.mb-0 Emprenveg
      // Icon Divider
      .divider-custom.divider-light
        .divider-custom-line
        .divider-custom-icon
          i.fas.fa-star
        .divider-custom-line
      // Masthead Subheading
      p.masthead-subheading.font-weight-light.mb-0 Administra tus información
  // Portfolio Section
  section#portfolio.page-section.portfolio
    .container
      // Portfolio Section Heading
      h2.page-section-heading.text-center.text-uppercase.text-secondary.mb-0 Editar información de tus productos
      // Icon Divider
      .divider-custom
        .divider-custom-line
        .divider-custom-icon
          i.fas.fa-star
        .divider-custom-line
      // Portfolio Grid Items
      .row.justify-content-center
        // Portfolio Item 1
        .col-md-6.col-lg-4.mb-5
          .portfolio-item.mx-auto(data-toggle='modal' data-target='#portfolioModal')
            .portfolio-item-caption.d-flex.align-items-center.justify-content-center.h-100.w-100
              .portfolio-item-caption-content.text-center.text-white
                i.fas.fa-plus.fa-3x
            img.img-fluid(src='/assets/img/portfolio/circus.png' alt='foto 1')
        // Portfolio Item 2
        .col-md-6.col-lg-4.mb-5
          .portfolio-item.mx-auto(data-toggle='modal' data-target='#portfolioModal')
            .portfolio-item-caption.d-flex.align-items-center.justify-content-center.h-100.w-100
              .portfolio-item-caption-content.text-center.text-white
                i.fas.fa-plus.fa-3x
            img.img-fluid(src='assets/img/portfolio/circus.png' alt='foto 2')
        // Portfolio Item 3
        .col-md-6.col-lg-4.mb-5
          .portfolio-item.mx-auto(data-toggle='modal' data-target='#portfolioModal')
            .portfolio-item-caption.d-flex.align-items-center.justify-content-center.h-100.w-100
              .portfolio-item-caption-content.text-center.text-white
                i.fas.fa-plus.fa-3x
            img.img-fluid(src='assets/img/portfolio/circus.png' alt='foto 3')
        // Portfolio Item 4
        .col-md-6.col-lg-4.mb-5.mb-lg-0
          .portfolio-item.mx-auto(data-toggle='modal' data-target='#portfolioModal')
            .portfolio-item-caption.d-flex.align-items-center.justify-content-center.h-100.w-100
              .portfolio-item-caption-content.text-center.text-white
                i.fas.fa-plus.fa-3x
            img.img-fluid(src='assets/img/portfolio/circus.png' alt='foto 4')
        // Portfolio Item 5
        .col-md-6.col-lg-4.mb-5.mb-md-0
          .portfolio-item.mx-auto(data-toggle='modal' data-target='#portfolioModal')
            .portfolio-item-caption.d-flex.align-items-center.justify-content-center.h-100.w-100
              .portfolio-item-caption-content.text-center.text-white
                i.fas.fa-plus.fa-3x
            img.img-fluid(src='assets/img/portfolio/circus.png' alt='foto 5')
        // Portfolio Item 6
        .col-md-6.col-lg-4
          .portfolio-item.mx-auto(data-toggle='modal' data-target='#portfolioModal')
            .portfolio-item-caption.d-flex.align-items-center.justify-content-center.h-100.w-100
              .portfolio-item-caption-content.text-center.text-white
                i.fas.fa-plus.fa-3x
            img.img-fluid(src='assets/img/portfolio/circus.png' alt='foto 6')
  // Contact Section
  section#contact.page-section
    .container
      // Contact Section Heading
      h2.page-section-heading.text-center.text-uppercase.text-secondary.mb-0 Edita tu información
      // Icon Divider
      .divider-custom
        .divider-custom-line
        .divider-custom-icon
          i.fas.fa-star
        .divider-custom-line
      // Contact Section Form
      .row
        .col-lg-8.mx-auto
          // To configure the contact form email address, go to mail/contact_me.php and update the email address in the PHP file on line 19.
          form#contactForm(name='sentMessage' novalidate='novalidate')
            .control-group
              .form-group.floating-label-form-group.controls.mb-0.pb-2
                label Name
                input#name.form-control(type='text' placeholder='Nombre de tu negocio')
                p.help-block.text-danger
            .control-group
              .form-group.floating-label-form-group.controls.mb-0.pb-2
                label Email 
                input#email.form-control(type='email' placeholder='Email')
                p.help-block.text-danger
            .control-group
              .form-group.floating-label-form-group.controls.mb-0.pb-2
                label Teléfono de contacto
                input#phone.form-control(type='tel' placeholder='Número de contacto')
                p.help-block.text-danger
            .control-group
              .form-group.floating-label-form-group.controls.mb-0.pb-2
                label Descripcion
                textarea#message.form-control(rows='5' placeholder='Descripción de tu emprendimiento')
                p.help-block.text-danger
            br
            #success
            .form-group
              button#sendMessageButton.btn.btn-primary.btn-xl(type='submit') Subir Info
  section.page-section
    .container
      h2.page-section-heading.text-center.text-uppercase.text-secondary.mb-5 Zona De Entregas
      #mapid            
  // Footer
  footer.footer.text-center
    .container
      .row
        // Footer Location
        .col-lg-4.mb-5.mb-lg-0
          h4.text-uppercase.mb-4 Ubicación
          p.lead.mb-0
            | Santiago Centro
            br
            |                             Santiago, Chile
        // Footer Social Icons
        .col-lg-4.mb-5.mb-lg-0
          h4.text-uppercase.mb-4 Sitios web
          a.btn.btn-outline-light.btn-social.mx-1(href='#!')
            i.fab.fa-fw.fa-facebook-f
          a.btn.btn-outline-light.btn-social.mx-1(href='#!')
            i.fab.fa-fw.fa-twitter
          a.btn.btn-outline-light.btn-social.mx-1(href='#!')
            i.fab.fa-fw.fa-linkedin-in
          a.btn.btn-outline-light.btn-social.mx-1(href='#!')
            i.fab.fa-fw.fa-dribbble
        // Footer About Text
        .col-lg-4
          h4.text-uppercase.mb-4 Sobre Emprenveg
          p.lead.mb-0
            | Emprenveg ha sido desarrollado para ayudar a emprendedores veganos
            | Únete a nuestra gran comunidad                            
  // Copyright Section
  .copyright.py-4.text-center.text-white
    .container
      small Copyright &copy; Your Website 2020
  // Scroll to Top Button (Only visible on small and extra-small screen sizes)
  .scroll-to-top.d-lg-none.position-fixed
    a.js-scroll-trigger.d-block.text-center.text-white.rounded(href='#page-top')
      i.fa.fa-chevron-up
  // Portfolio Modals
  // Portfolio Modal 
  include formModal
  
  // Bootstrap core JS
  script(src='https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js')
  script(src='https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.bundle.min.js')
  // Third party plugin JS
  script(src='https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.4.1/jquery.easing.min.js')
  // Contact form JS
  // Core theme JS
  script(src='/javascripts/scripts.js')
  script(src='/javascripts/map.js')



